{
  "name": "mopower-ui",
  "version": "0.0.0",
  "description": "A Component Library from mp",
  "private": true,
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "types/index.d.ts",
  "unpkg": "lib/index.js",
  "style": "lib/theme-chalk/index.css",
  "scripts": {
    "build": "yarn clean:lib && yarn build:esm-bundle && yarn build:lib && yarn build:components  && yarn build:util && yarn build:theme",
    "bootstrap": "yarn && npx lerna bootstrap",
    "examples-dev": "webpack-dev-server --config ./examples/webpack.config.js",
    "clean:lib": "rimraf lib && rimraf es",
    "build:esm-bundle": "cross-env LIBMODE=esm rollup --config ./build/rollup.config.bundle.js",
    "build:lib": "rollup --config ./build/rollup.config.bundle.js",
    "build:components": "rollup --config ./build/rollup.config.js",
    "build:theme": "rimraf packages/theme-chalk/lib && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk && rimraf packages/theme-chalk/lib",
    "build:util": "cross-env BABEL_ENV=utils babel packages/utils --out-dir lib/utils",
    "publish:major": "cross-env VERSION_TYPE=major node ./build/publish.js",
    "publish:minor": "cross-env VERSION_TYPE=minor node ./build/publish.js",
    "publish:patch": "cross-env VERSION_TYPE=patch node ./build/publish.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^4.2.2",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^0.11.2",
    "sass": "^1.35.1",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.14",
    "vue": "^2.6.14",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "lerna": "^3.22.1",
    "rollup": "^2.28.2",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-esbuild": "^4.2.3",
    "esbuild": "^0.12.5",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "cross-env": "^7.0.2",
    "cp-cli": "^2.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "file-loader": "^6.0.0"
  },
  "peerDependencies": {
    "vue": "^2.6.x"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "pdfjs-dist": "^2.8.335",
    "@popperjs/core": "^2.9.2",
    "element-ui": "2.13.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}